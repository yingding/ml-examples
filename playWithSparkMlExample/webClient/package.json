{
  "name": "web-client",
  "perferGlobal": false,
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080",
    "local-proxy": "ng serve --port 8080 --proxyConfig src/config/local.proxy.json",
    "build": "rimraf $npm_package_buildPath && ng build --outputPath=$npm_package_buildPath",
    "deploy": "rimraf $npm_package_deployPath && ng build --outputPath=$npm_package_deployPath",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "buildPath": "dist",
  "deployPath": "../server/public",
  "comments": "windows use %npm_package_x instead $npm_package_x to reference to a package.json variable, reference: https://stackoverflow.com/questions/43705195/how-can-i-use-variables-in-package-json",
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.14",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.14",
    "@angular/compiler": "~7.2.14",
    "@angular/core": "~7.2.14",
    "@angular/forms": "~7.2.14",
    "@angular/http": "~7.2.14",
    "@angular/material": "~7.3.7",
    "@angular/platform-browser": "~7.2.14",
    "@angular/platform-browser-dynamic": "~7.2.14",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "moment": "^2.24.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "rimraf": "~2.6.3",
    "tar": "~4.4.8",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
